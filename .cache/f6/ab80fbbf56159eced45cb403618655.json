 0;\r\n     border-radius: 50px;\r\n     cursor: pointer;\r\n     margin-right: 7px;\r\n     outline: none;\r\n\r\n    &:checked::before {\r\n      position: absolute;\r\n      font: 13px/1 'Open Sans', sans-serif;\r\n      left: 11px;\r\n      top: 7px;\r\n      content: '\\02143';\r\n      transform: rotate(40deg);\r\n    }\r\n\r\n    &:hover {\r\n      background-color: #f7f7f7;\r\n    }\r\n\r\n    &:checked{\r\n      background-color: #f1f1f1;\r\n    }\r\n}\r\n\r\n.radio-label {\r\n  font: 15px/1.7 'Open Sans', sans-serif;\r\n  color: #333;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  cursor: pointer;\r\n}\r\n\r\n\r\n.btn{\r\n  border: none;\r\n  color: white;\r\n  background: var(--light-blue);\r\n\r\n  &:hover{\r\n    cursor: pointer;\r\n    color: var(--dark-blue);\r\n    background: white;\r\n    border: 1px solid var(--light-blue);\r\n  }\r\n\r\n  &:disabled {\r\n    background: #aaa;\r\n  }\r\n}\r\n\r\n.select {\r\n  width: 200px;\r\n  border: none;\r\n  border-bottom: 1px solid var(--light-blue);\r\n  margin: 6px;\r\n  padding: 4px;\r\n  background: var(--light-blue);\r\n  color: white;\r\n\r\n  &:hover {\r\n    background: white;\r\n    color: var(--dark-blue);\r\n  }\r\n\r\n  .option {\r\n    height: 2em;\r\n  }\r\n}\r\n\r\n.link {\r\n  color: white;\r\n  font-size: 15px;\r\n  font-weight: bold;\r\n  margin: 6px;\r\n\r\n  &:hover {\r\n    color: white;\r\n  }\r\n}\r\n\r\n.input {\r\n  padding: 0 6px;\r\n}\r\n\r\nprogress {\r\n  transform: rotate(180deg);\r\n}\r\n\r\n.group {\r\n  width: 100px;\r\n}\r\n\r\n.name {\r\n  padding: 3px;\r\n  margin: 3px;\r\n  font-weight: bold;\r\n}\r\n\r\n.inspect {\r\n  width: 20px;\r\n  height: 20px;\r\n  text-align: center;\r\n\r\n  &:hover {\r\n    color: var(--light-blue);\r\n    background: white;\r\n    border-radius: 20%;\r\n  }\r\n}\r\n\r\n\r\n.tagline {\r\n  quotes: \"“\" \"”\" \"‘\" \"’\";\r\n  text-align: center;\r\n\r\n  &::before {\r\n    content: open-quote;\r\n  }\r